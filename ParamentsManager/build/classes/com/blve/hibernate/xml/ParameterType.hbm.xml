<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.blve.hibernate.pojo.ParameterType" table="`PARAMETER_TYPE_TBL`">
        <id name="parameterTypeID" type="int">
            <column name="`ParameterTypeID`" />
            <generator class="sequence">
                <param name="sequence">PARAMETER_TYPE_SEQ</param>
            </generator>
        </id>
        <property name="parameterType" type="java.lang.String">
            <column name="`ParameterType`" />
        </property>
        <property name="parameterTypeDescription" type="java.lang.String">
            <column name="`ParameterTypeDescription`" />
        </property>
    </class>
</hibernate-mapping>
